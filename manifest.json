{
	"name": "EasyTest Form Filler",
	"version": "1.0",
	"description": "Automatic form filler that fills all fields on webpage with random data.",
	"permissions": ["activeTab"],
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
	}],
	"icons": {
		"128": "/images/128.png"
	}
}
